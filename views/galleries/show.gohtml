{{define "yield"}}
  <div class="row">
    <div class="col-md-12">
      <h1>{{.Title}}</h1>
    </div>
  </div>
  <div class="row">
    {{range .ImagesSplitN 3}}
      <div class="col-md-4">
        {{range .}}
          <a href="{{.}}">
            <img src="{{.}}" alt="" class="img-responsive">
          </a>
        {{end}}
      </div>
    {{end}}
  </div>
{{end}}
